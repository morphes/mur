<page layout="3columns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="default_head_blocks"/>
    <head>
        <css src="css/styles-m.css"/>
        <css src="css/styles-l.css" media="screen and (min-width: 768px)"/>
        <meta name="format-detection" content="telephone=no"/>
        <css src='css/styles-murata.css' media='screen'/>
        <css src="css/media.css" media="all"/>
        <css src="css/jquery.mmenu.all.css"/>
        <css src="css/additional.css"/>
        <css src="css/fixies.css"/>
        <link src="js/calculator.js"/>
    </head>
    <body>
        <referenceBlock name="head.additional">
            <block class="Magento\Framework\View\Element\Template" name="frontuser" template="Murata_Theme::extend/bootstrapcdn.phtml"/>
        </referenceBlock>
        <referenceContainer name="columns.top" remove="true"/>
        <referenceContainer name="root" htmlTag="div" htmlClass="wrapper">
            <container name="main-wrapper-page" htmlTag="div" htmlClass="page">
                <referenceContainer name="header.container">
                    <container name="header.panel.wrapper" htmlTag="div" htmlClass="top-nav-rfm" before="-">
                        <container name="header.panel">
                            <referenceBlock name="store_language" remove="true"/>
                            <referenceBlock name="top.links" remove="true"/>
                            <block class="Magento\Framework\View\Element\Template" name="skip_to_content" template="Murata_Theme::murata-theme/html/header-line.phtml"/>
                        </container>
                    </container>
                    <container name="header-wrapper" label="Page Header" as="header-wrapper" htmlTag="div" htmlClass="header-container asdf324 page-header">
                        <container name="header-wrapper-container" label="Page Header" as="header-wrapper-container" htmlTag="div" htmlClass="header page-header">

                            <block class="Murata\Theme\Block\Html\Header\Logo" name="logo" before="-">
                                <arguments>
                                    <argument name="logo_img_width" xsi:type="number">111</argument>
                                    <argument name="logo_img_height" xsi:type="number">222</argument>
                                </arguments>
                            </block>
                            <block class="Murata\Theme\Block\Html\GeoPlugin" name="geoplugin" after="logo"/>
                            <container name="nav-container" label="Nav Container" as="nav-container" htmlTag="div" htmlClass="nav-container container">
                                <block class="Magento\Framework\View\Element\Template" name="nav.sections" after="header-wrapper-row" template="Murata_Theme::html/sections.phtml">
                                    <arguments>
                                        <argument name="group_name" xsi:type="string">navigation-sections</argument>
                                        <argument name="group_css" xsi:type="string">nav-sections</argument>
                                    </arguments>
                                    <block class="Magento\Framework\View\Element\Template" name="store.menu" group="navigation-sections" template="Magento_Theme::html/container.phtml">
                                        <arguments>
                                            <argument name="title" translate="true" xsi:type="string">Menu</argument>
                                        </arguments>
                                        <block class="Magento\Theme\Block\Html\Topmenu" name="catalog.topnav" template="Magento_Theme::html/topmenu.phtml" ttl="3600" before="-"/>
                                    </block>
                                    <block class="Magento\Framework\View\Element\Text" name="store.links" group="navigation-sections">
                                        <arguments>
                                            <argument name="title" translate="true" xsi:type="string">Account</argument>
                                            <argument name="use_force" xsi:type="boolean">true</argument>
                                            <argument name="text" xsi:type="string"><![CDATA[<!-- Account links -->]]></argument>
                                        </arguments>
                                    </block>
                                    <block class="Magento\Framework\View\Element\Template" name="store.settings" group="navigation-sections" template="Magento_Theme::html/container.phtml">
                                        <arguments>
                                            <argument name="title" translate="true" xsi:type="string">Settings</argument>
                                        </arguments>
                                        <block class="Magento\Store\Block\Switcher" name="store.settings.language" template="Magento_Store::switch/languages.phtml">
                                            <arguments>
                                                <argument name="id_modifier" xsi:type="string">nav</argument>
                                            </arguments>
                                        </block>
                                        <block class="Magento\Directory\Block\Currency" name="store.settings.currency" template="Magento_Directory::currency.phtml">
                                            <arguments>
                                                <argument name="id_modifier" xsi:type="string">nav</argument>
                                            </arguments>
                                        </block>
                                    </block>
                                </block>
                                <block class="Murata\Theme\Block\Html\Dropdown" name="murataps.dropdown" template="Murata_Theme::html/header/dropdown.phtml"></block>
                            </container>
                            <block class="Murata\Theme\Block\Html\Search\Form" name="top.search" as="topSearch" template="Murata_Theme::html/search/form.mini.phtml" />
                        </container>
                        <container name="header.panel.after.shadow.in.header" htmlTag="div" htmlClass="header_bar_rfm double"  after="header-wrapper">
                            <block class="Magento\Framework\View\Element\Template" name="header.bar.rfm" template="Murata_Theme::html/empty.phtml"/>
                        </container>
                    </container>
                    <container name="header.panel.after.shadow" htmlTag="div" htmlClass="header_bar_rfm"  after="header-wrapper">
                        <block class="Magento\Framework\View\Element\Template" name="header.bar.rfm" template="Murata_Theme::html/empty.phtml"/>
                    </container>
                </referenceContainer>
                <referenceContainer name="footer-container" htmlTag="div" htmlClass="footer">
                    <referenceContainer name="footer" remove="true"/>
                    <container name="top-footer" as="top-footer" label="Top Footer" htmlTag="div" htmlClass="footer-top">
                        <block class="Murata\Theme\Block\Html\Footer" name="top_footer" as="top_footer" template="Murata_Theme::html/footer/top.phtml"/>
                    </container>
                    <container name="main-footer" as="main-footer" label="Main Footer" htmlTag="div" htmlClass="footer-main">
                        <block class="Murata\Theme\Block\Html\Footer" name="main_footer" as="main_footer" template="Murata_Theme::html/footer/main.phtml"/>
                    </container>
                    <container name="bottom-footer" as="bottom-footer" label="Bottom Footer" htmlTag="div" htmlClass="footer-bottom">
                        <block class="Murata\Theme\Block\Html\Footer" name="bottom_footer" as="bottom_footer" template="Murata_Theme::html/footer/bottom.phtml"/>
                    </container>
                </referenceContainer>
                <referenceContainer name="page.top">
                    <referenceContainer name="navigation.sections" remove="true"/>
                    <container name="top.container" as="topContainer" label="After Page Header Top" htmlTag="div" htmlClass="top-container"/>
                </referenceContainer>
                <referenceBlock name="copyright" remove="true"/>

            </container>
        </referenceContainer>
        <referenceContainer name="page.wrapper">
            <block class="Bss\Megamenu\Block\Html\MobileMenu" name="bss_megamenu_mobile" template="Bss_Megamenu::murata_theme/html/mobile_menu.phtml">
                <block class="Murata\Theme\Block\Html\Dropdown" name="menu.murataps.dropdown" template="Murata_Theme::html/header/dropdown_mobile.phtml"></block>
            </block>
            <block class="Magento\Framework\View\Element\Template" template="Murata_Theme::html/murata_js.phtml" name="before_body_js" after="-"/>
        </referenceContainer>
        <referenceContainer name="columns.main.main">
            <container name="page.messages" htmlTag="div" htmlClass="page messages" after="breadcrumbs">
                <block class="Magento\Framework\View\Element\Template" name="ajax.message.placeholder" template="Magento_Theme::html/messages.phtml"/>
                <block class="Magento\Framework\View\Element\Messages" name="messages" as="messages" template="Magento_Theme::messages.phtml"/>
            </container>
        </referenceContainer>
    </body>
</page>