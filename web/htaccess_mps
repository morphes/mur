AllowEncodedSlashes On

<IfModule mod_rewrite.c>
## enable rewrites
        Options +FollowSymLinks
        RewriteEngine on
        RewriteCond %{REQUEST_URI} ^/(.+)/where/(.+)
        RewriteRule ^/(.+)/where/(.+)$ /$1.html [R=301,L]

